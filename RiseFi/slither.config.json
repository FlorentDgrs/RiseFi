{
  "filter_paths": ["lib/", "node_modules/", "test/", "script/"],
  "exclude_informational": true,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_dependencies": true,
  "exclude_optimization": true,
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "naming-convention",
    "too-many-digits",
    "unused-state",
    "calls-loop",
    "reentrancy-benign",
    "reentrancy-events",
    "external-function",
    "low-level-calls",
    "unchecked-lowlevel",
    "unchecked-send",
    "arbitrary-send",
    "controlled-array-length",
    "assembly",
    "constable-states",
    "immutable-states",
    "var-read-using-this"
  ],
  "printers_to_run": "",
  "disable_color": false
}
