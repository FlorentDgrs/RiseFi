{
  "name": "risefi-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run build",
    "clean": "rm -rf .next out",
    "anvil": "cd ../RiseFi && ./scripts/start-anvil.sh",
    "deploy": "cd ../RiseFi && ./scripts/fund-and-deploy.sh",
    "stack": "concurrently \"npm run anvil\" \"wait-on http://localhost:8545 && npm run deploy\" \"wait-on http://localhost:8545 && npm run dev\"",
    "generate-abi": "node scripts/generate-abi.js",
    "build-contracts": "cd ../RiseFi && forge build --force && cd ../frontend && npm run generate-abi"
  },
  "dependencies": {
    "@morpho-org/blue-sdk": "^4.2.0",
    "@morpho-org/simulation-sdk": "^3.1.1",
    "@noble/hashes": "^1.8.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.31.7",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "wait-on": "^7"
  }
}
